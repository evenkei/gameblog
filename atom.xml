---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Respond Rate Blog</title>
	<link href="https://evenkei.github.io/respondrate/atom.xml" rel="self"/>
	<link href="https://evenkei.github.io/respondrate/"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://evenkei.github.io/respondrate</id>
	<author>
		<name>Marina Kittaka</name>
		<email>marinakittaka@gmail.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="https://evenkei.github.io/respondrate{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>https://evenkei.github.io/respondrate{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>